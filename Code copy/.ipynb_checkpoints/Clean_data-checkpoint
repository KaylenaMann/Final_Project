   ],
   "source": [
    "model = LogisticRegressionCV(\n",
    "    Cs=10,\n",
    "    cv=5,\n",
    "    penalty=\"l2\",\n",
    "    solver=\"lbfgs\",\n",
    "    class_weight=\"balanced\",\n",
    "    scoring=\"roc_auc\",\n",
    "    max_iter=5000,\n",
    "    random_state=42\n",
    ")\n",
    "\n",
    "model.fit(X_train_scaled, y_train)\n",
    "\n",
    "# Validation performance\n",
    "valid_auc = roc_auc_score(y_valid, model.predict_proba(X_valid_scaled)[:, 1])\n",
    "print(f\"Validation ROC AUC (2017): {valid_auc:.3f}\")\n",
    "\n",
    "y_valid_pred = model.predict(X_valid_scaled)\n",
    "print(f\"\\nValidation Metrics:\")\n",
    "print(f\"Recall:    {recall_score(y_valid, y_valid_pred):.3f}\")\n",
    "print(f\"Precision: {precision_score(y_valid, y_valid_pred):.3f}\")\n",
    "print(f\"F1 Score:  {f1_score(y_valid, y_valid_pred):.3f}\")\n",
    "\n",
    "# Test performance\n",
    "test_auc = roc_auc_score(y_test, model.predict_proba(X_test_scaled)[:, 1])\n",
    "print(f\"\\nTest ROC AUC (2018): {test_auc:.3f}\")\n",
    "\n",
    "y_test_pred = model.predict(X_test_scaled)\n",
    "print(f\"\\nTest Metrics:\")\n",
    "print(f\"Recall:    {recall_score(y_test, y_test_pred):.3f}\")\n",
    "print(f\"Precision: {precision_score(y_test, y_test_pred):.3f}\")\n",
    "print(f\"F1 Score:  {f1_score(y_test, y_test_pred):.3f}\")\n",
    "\n",
    "tuned_model = TunedThresholdClassifierCV(\n",
    "    model, \n",
    "    scoring=\"f1\",\n",
    "    cv=5\n",
    ")\n",
    "tuned_model.fit(X_train_scaled, y_train)\n",
    "\n",
    "print(f\"Optimal threshold: {tuned_model.best_threshold_:.3f}\")"
   ]
