    "cutoff = train[\"math_test_pct_prof_midpt\"].dropna().quantile(0.25)  \n",
    "\n",
    "for df in [train, valid, test]:\n",
    "\n",
    "    df[\"y\"] = (df[\"math_test_pct_prof_midpt\"] <= cutoff).astype(int)\n",
    "\n",
    "train = train.dropna(subset=[\"y\"])\n",
    "valid = valid.dropna(subset=[\"y\"])\n",
    "test  = test.dropna(subset=[\"y\"])"
   ]
